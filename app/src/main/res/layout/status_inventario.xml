    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="85dp"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#000000"
            android:text="@string/estado"/>

        <TextView
            android:layout_width="85dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Fecha"
            android:textColor="#000000" />

        <TextView
            android:layout_width="85dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Hora"
            android:textColor="#000000" />

        <TextView
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Local"
            android:textColor="#000000" />

        <TextView
            android:layout_width="95dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Usuario"
            android:textColor="#000000" />

        <TextView
            android:layout_width="95dp"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#000000"
