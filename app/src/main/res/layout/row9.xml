<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="85dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Estado"
            android:textColor="#000000" />

        <TextView
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Fecha"
            android:textColor="#000000" />

        <TextView
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Hora"
            android:textColor="#000000" />

        <TextView
            android:layout_width="110dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Codigo"
            android:textColor="#000000"
            android:visibility="gone"/>

        <TextView
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Nombre Comercial"
            android:textColor="#000000" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Categoria"
            android:textColor="#000000" />

        <TextView
            android:layout_width="75dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Marca"
            android:textColor="#000000" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Tipo Logro"
            android:textColor="#000000" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Status"
            android:textColor="#000000" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Observacion"
            android:textColor="#000000" />


    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/lblEstado"
            android:layout_width="85dp"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#676666"
            android:text="Estado"/>

        <TextView
            android:id="@+id/lblFecha"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="codigo"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblHora"
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Usuario"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblCodigo"
            android:layout_width="110dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="codigo"
            android:textColor="#676666"
            android:visibility="gone"/>



        <TextView
            android:id="@+id/lblNombrecomercial"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Nombre Comercial"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblCategoria"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Categoria"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblMarca"
            android:layout_width="75dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Marca"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblTipoLogro"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Tipo Logro"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblStatus"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Status"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblObservacion"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Observacion"
            android:textColor="#676666" />




    </LinearLayout>

</LinearLayout>