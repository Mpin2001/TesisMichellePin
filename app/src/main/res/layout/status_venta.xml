<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white"
    android:orientation="vertical" >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

                <TextView
                    android:layout_width="85dp"
                    android:layout_height="match_parent"
                    android:ems="5"
                    android:background="@null"
                    android:padding="5dp"
                    android:textColor="#000000"
                    android:text="@string/estado"/>

            <TextView
                android:layout_width="90dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="@string/fecha"
                android:textColor="#000000" />

            <TextView
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="@string/hora"
                android:textColor="#000000" />

            <TextView
                android:layout_width="85dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="Código"
                android:textColor="#000000" />

            <TextView
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="Tipo Factura"
                android:textColor="#000000" />

            <TextView
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="Número Factura"
                android:textColor="#000000" />

            <TextView
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="Monto Factura Huggies"
                android:textColor="#000000" />

            <TextView
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="Fecha Venta"
                android:textColor="#000000" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


                <TextView
                    android:id="@+id/lblEstado"
                    android:layout_width="85dp"
                    android:layout_height="match_parent"
                    android:ems="5"
                    android:background="@null"
                    android:padding="5dp"
                    android:textColor="#676666"
                    android:text="@string/estado"/>

            <TextView
                android:id="@+id/lblfecha"
                android:layout_width="90dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="@string/fecha"
                android:textColor="#676666" />

            <TextView
                android:id="@+id/lblhora"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="@string/hora"
                android:textColor="#676666" />

            <TextView
                    android:id="@+id/lblCategoria"
                    android:layout_width="85dp"
                    android:layout_height="match_parent"
                    android:ems="5"
                    android:background="@null"
                    android:padding="5dp"
                    android:textColor="#676666"
                    android:text="@string/categoria"/>

            <TextView
                android:id="@+id/lblsubcat"
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="@string/subcat"
                android:textColor="#676666" />

            <TextView
                android:id="@+id/lblmarca"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="@string/status"
                android:textColor="#676666" />

            <TextView
                android:id="@+id/lblproducto"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:ems="5"
                android:padding="5dp"
                android:text="@string/producto"
                android:textColor="#676666" />

                <TextView
                    android:id="@+id/lbltotal"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:ems="5"
                    android:background="@null"
                    android:padding="5dp"
                    android:textColor="#676666"
                    android:text="@string/total"/>


        </LinearLayout>

</LinearLayout>