<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="85dp"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#000000"
            android:text="@string/estado"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Fecha"
            android:textColor="#000000" />

        <TextView
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Hora"
            android:textColor="#000000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Codigo"
            android:textColor="#000000" />

        <TextView
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Usuario"
            android:textColor="#000000" />

        <TextView
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Local"
            android:textColor="#000000" />

        <TextView
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Código Fabril"
            android:textColor="#000000" />

        <TextView
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Vendedor Fabril"
            android:textColor="#000000" />

        <TextView
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Categoría"
            android:textColor="#000000" />

        <TextView
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Subategoria"
            android:textColor="#000000" />

        <TextView
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Brand"
            android:textColor="#000000"
            android:visibility="gone"/>

        <TextView
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="SKU"
            android:textColor="#000000" />

        <TextView
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Quiebre"
            android:textColor="#000000" />

        <TextView
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Unidad Disponible"
            android:textColor="#000000" />

        <TextView
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Sugerido"
            android:textColor="#000000" />

        <TextView
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Cantidad"
            android:textColor="#000000" />

        <TextView
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Observaciones"
            android:textColor="#000000" />

        <TextView
            android:layout_width="110dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="Fecha"
            android:textColor="#000000" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/cantidad"
            android:textColor="#000000" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/lblEstado"
            android:layout_width="85dp"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#676666"
            android:text="@string/estado"/>

        <TextView
            android:id="@+id/lblfechainicial"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#676666"
            android:text="@string/fechainicio"/>

        <TextView
            android:id="@+id/lblMara"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#676666"
            android:text="@string/status" />

        <TextView
            android:id="@+id/lblCategoria"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/categoria"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblTipo"
            android:layout_width="65dp"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#676666"
            android:text="@string/tipo"/>

        <TextView
            android:id="@+id/lblLocal"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#676666"
            android:text="@string/local"/>

        <TextView
            android:id="@+id/lblCodigoFabril"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#676666"
            android:text="@string/codigo_fabril"/>

        <TextView
            android:id="@+id/lblVendedor"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:ems="5"
            android:background="@null"
            android:padding="5dp"
            android:textColor="#676666"
            android:text="@string/vendedor_fabril"/>

        <TextView
            android:id="@+id/lblProductos"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/productos"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblSub"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/productos"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblCantidad"
            android:layout_width="70dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/cantidad"
            android:textColor="#676666"
            android:visibility="gone"/>

        <TextView
            android:id="@+id/lblPcont"
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/pcont"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblQuiebre"
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/quiebre"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblUDisponible"
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/Udisponible"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblSugerido"
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/Csugerido"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblCCantidad"
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/Ccantidad"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblObservaciones"
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/Sobservaciones"
            android:textColor="#676666" />

        <TextView
            android:id="@+id/lblFecha"
            android:layout_width="110dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/fecha"
            android:textColor="#676666"
            android:visibility="gone"/>

        <TextView
            android:id="@+id/lblHora"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:ems="5"
            android:padding="5dp"
            android:text="@string/hora"
            android:textColor="#676666"
            android:visibility="gone"/>

    </LinearLayout>

</LinearLayout>